package Practice;
import java.io.FileNotFoundException;
import java.nio.file.FileAlreadyExistsException;

import Practice.*;

public class Advance_Calculator extends Calculator implements MyInterface{

	int i=11;
	public int mul(int x, int y)			//inheritance
	{  return x*y;  }
	
	public double div(int x, int y) {		//Interface definition
		return x/y;
	}
	
	public int sum(int x, int y) 			//function overriding 
	{ 
	
		return x+y ;		
	}

	

}
