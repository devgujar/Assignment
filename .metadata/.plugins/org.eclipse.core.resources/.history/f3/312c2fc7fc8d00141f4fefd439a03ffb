package Experimental.ReadFromExcell;

import java.io.File;
import java.io.FileInputStream;
import java.io.IOException;
import jxl.Sheet;
import jxl.Workbook;
import jxl.read.biff.BiffException;


public class ReadFromExcell {

	public static void main(String[] args) throws BiffException, IOException {			

		Workbook wb = Workbook.getWorkbook(new File("D://Automation/workspace/myproject/src/Experimental/ReadFromExcell/myexcell.xls"));
		Sheet sh = wb.getSheet("Sheet1");
		
		int rows = sh.getRows();
		int cols = sh.getColumns();
		System.out.print("\nrows:"+rows+"\nColumns:"+cols+"\n");
		int row=0;
		
		System.out.print(sh.getCell(0,0).getContents()+"::");
		System.out.print(sh.getCell(0,1).getContents()+"\n");

		System.out.print(sh.getCell(1,0).getContents()+"::");
		System.out.print(sh.getCell(1,1).getContents()+"\n");

		System.out.print(sh.getCell(2,0).getContents()+"::");

	}
	

}